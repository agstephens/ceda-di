{
  "eufar": {
    "_all": {
        "enabled": true
    },
    "_id" {
        "path": "_id"
    }
    "properties": {
      "data_format": {
        "properties": {
          "format": {
            "type": "string",
            "store": "yes"
          }
        }
      },
      "file": {
        "properties": {
          "filename": {
            "type": "string",
            "analyzer": "simple",
            "store": "yes"
          },
          "path": {
            "type": "string",
            "analyzer": "simple",
            "store": "yes"
          },
          "size": {
            "type": "long"
          }
        }
      },
      "misc": {
        "type": "object"
      },
      "spatial": {
        "properties": {
          "geometries": {
            "properties": {
              "bbox": {
                "type": "geo_shape",
                "tree": "quadtree",
                "precision": "50m",
                "store": "yes"
              },
              "summary": {
                "properties": {
                  "coordinates": {
                    "type": "double",
                    "enabled": false,
                    "include_in_all" : false,
                    "index": "no",
                    "store": "yes"
                  }
                }
              }
            }
          }
        }
      },
      "temporal": {
        "properties": {
          "end_time": {
            "type": "date",
            "format": "dateOptionalTime",
            "store": "yes"
          },
          "start_time": {
            "type": "date",
            "format": "dateOptionalTime",
            "store": "yes"
          }
        }
      }
    }
  }
}
